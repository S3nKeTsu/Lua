M:AddButton({
	Name = "Anti Ban from houses",
	Icon = "",
	Callback = function(bn)
    OrionLib:MakeNotification({
	Name = "Warning!",
	Content = "Rooms will not load when entering a house!",
	Time = 4
	})
local bn = getrawmetatable(game)
make_writeable(bn)

local namecall = bn.__namecall

bn.__namecall = newcclosure(function(self, ...)
    local method = getnamecallmethod()
    local args = {...}

    if method == "FireServer" and tostring(self) == "PlayerChannel" then
       return
    end
    return namecall(self, table.unpack(args))
end)


  	end    
})

M:AddLabel("Work at the Pizza Place Chat Spam")
M:AddTextbox({
	Name = "Chat Spam",
	TextDisappear = true,
	Icon = "rbxassetid://5654616444",
	Callback = function(txt)
	OrionLib:MakeNotification({
	Name = "Chat",
	Content = "the spamming is on!",
	Time = 4
	})
    _G.Enabled = true 
    local msg = txt 
	   
 
    if _G.Enabled == true then
    while _G.Enabled do
    wait(0.4)
    game.Workspace.Main.Chatted:FireServer(msg)
    end
    end	

	end	 

})

M:AddButton({
	Name = "Stop",
	Icon = "",
	Callback = function(txt)
    OrionLib:MakeNotification({
	Name = "Chat",
	Content = "the spamming is Off!",
	Time = 4
	})
	_G.Enabled = false 
    local msg = txt 
	   
 
    if _G.Enabled == true then
    while _G.Enabled do
    wait(0.4)
    game.Workspace.Main.Chatted:FireServer(msg)
    end
    end	
  	end    
