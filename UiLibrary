

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Pizza Place Hub", HidePremium = true, SaveConfig = false, ConfigFolder = "Pizza Hub", IntroEnabled = true, IntroText = "Work at the Pizza Place /Pizza Place Hub", })



OrionLib:MakeNotification({
	Name = "Ui Library",
	Content = "YES!! it's here, that new ui library!",
	Time = 5
})












local M = Window:MakeTab({
	Name = "Main",
	PremiumOnly = false
})

local S = Window:MakeTab({
	Name = "Scripts",
	PremiumOnly = false
})

local T = Window:MakeTab({
	Name = "Teleports",
	PremiumOnly = false
})

local I = Window:MakeTab({
	Name = "Info",
	PremiumOnly = false
})
local Section = M:AddSection({
	Name = "Main"
})

--Main--

M:AddParagraph("Commands Creator's","Vin#9999 and lil squeasy#1625")

 M:AddButton({
    Name = "Commands",
    Icon = "0",
    Callback = function()
    spawn(loadstring(game:HttpGet("https://raw.githubusercontent.com/incognitototo/sda/main/dwa.lua",true)))
end
})

M:AddLabel("Work at the Pizza Place Chat Spam")
M:AddTextbox({
	Name = "Chat Spam",
	TextDisappear = true,
	Icon = "rbxassetid://5654616444",
	Callback = function(txt)
	OrionLib:MakeNotification({
	Name = "Chat",
	Content = "the spamming is on!",
	Time = 4
	})
    _G.Enabled = true 
    local msg = txt 
	   
 
    if _G.Enabled == true then
    while _G.Enabled do
    wait(0.4)
    game.Workspace.Main.Chatted:FireServer(msg)
    end
    end	

	end	 

})

M:AddButton({
	Name = "Stop",
	Icon = "",
	Callback = function(txt)
    OrionLib:MakeNotification({
	Name = "Chat",
	Content = "the spamming is Off!",
	Time = 4
	})
	_G.Enabled = false 
    local msg = txt 
	   
 
    if _G.Enabled == true then
    while _G.Enabled do
    wait(0.4)
    game.Workspace.Main.Chatted:FireServer(msg)
    end
    end	
  	end    
})

M:AddLabel("Slider's")
M:AddSlider({
	Name = "WalkSpeed",
	Min = 16,
	Max = 500,
	Default = 16,
	Increment = 5,
	ValueName = "WalkSpeed",
	Callback = function(W)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = W
	end    
})

M:AddSlider({
	Name = "JumpPower",
	Min = 50,
	Max = 500,
	Default = 50,
	Increment = 5,
	ValueName = "JumpPower",
	Callback = function(J)
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = J
	end    
})

M:AddSlider({
	Name = "Gravity",
	Min = 0,
	Max = 200,
	Default = 200,
	Increment = 5,
	ValueName = "Gravity",
	Callback = function(G)
	game.Workspace.Gravity = G
	end    
})

local Section = T:AddSection({
	Name = "Teleport's"
})
--Teleports
T:AddButton({
	Name = "Home",
	Icon = "",
	Callback = function()
      		local args = {
         [1] = "TeleportHome"
     }
     game:GetService("ReplicatedStorage").PlayerChannel:FireServer(unpack(args))
    
  	end    
})

T:AddButton({
	Name = "Starter Zone",
	Icon = "",
	Callback = function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(49, 3, 198)
    end
 
