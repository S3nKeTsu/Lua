--Teleports
T:AddButton({
	Name = "Home",
	Callback = function()
      		local args = {
         [1] = "TeleportHome"
     }
     game:GetService("ReplicatedStorage").PlayerChannel:FireServer(unpack(args))
    
  	end    
})

